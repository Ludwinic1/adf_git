{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf1-njl"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/Snowflake2')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "Snowflake",
				"annotations": [],
				"parameters": {
					"snowflake_db": {
						"type": "String",
						"defaultValue": "NEWDB"
					},
					"snowflake_wh": {
						"type": "string",
						"defaultValue": "COMPUTE_WH"
					}
				},
				"typeProperties": {
					"connectionString": "jdbc:snowflake://uv77166.east-us-2.azure.snowflakecomputing.com/?user=ludwinic1&db=@{linkedService().snowflake_db}&warehouse=@{linkedService().snowflake_wh}",
					"password": {
						"type": "SecureString",
						"value": "**********"
					}
				}
			},
			"dependsOn": []
		}
	]
}