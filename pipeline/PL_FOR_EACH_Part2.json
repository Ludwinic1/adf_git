{
	"name": "PL_FOR_EACH_Part2",
	"properties": {
		"activities": [
			{
				"name": "ForEach1",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [
					{
						"name": "Userprop1",
						"value": "SomeuserpropValue"
					}
				],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.mySinkDatasetFolderPath",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "MyCopyActivity",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "DelimitedTextSource",
									"storeSettings": {
										"type": "AzureBlobFSReadSettings",
										"recursive": false,
										"enablePartitionDiscovery": false
									},
									"formatSettings": {
										"type": "DelimitedTextReadSettings"
									}
								},
								"sink": {
									"type": "DelimitedTextSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings"
									},
									"formatSettings": {
										"type": "DelimitedTextWriteSettings",
										"quoteAllText": true,
										"fileExtension": ".txt"
									}
								},
								"enableStaging": false,
								"translator": {
									"type": "TabularTranslator",
									"typeConversion": true,
									"typeConversionSettings": {
										"allowDataTruncation": false,
										"treatBooleanAsNumber": false
									}
								}
							},
							"inputs": [
								{
									"referenceName": "ds3",
									"type": "DatasetReference",
									"parameters": {
										"MyFolderPath": {
											"value": "@pipeline().parameters.mySourceDataSetFolderPath",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "DS_FOR_EACH_OUTPUT",
									"type": "DatasetReference"
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"mySinkDatasetFolderPath": {
				"type": "array",
				"defaultValue": [
					"ForEachOutput/file1",
					"forEachOutput/file2"
				]
			},
			"mySourceDataSetFolderPath": {
				"type": "string",
				"defaultValue": "ForEachInput/"
			}
		},
		"folder": {
			"name": "ForEach"
		},
		"annotations": []
	}
}